@model GymMananger.Web.Models.MemberListViewModel

@{

    Layout = "~/Views/Shared/_Layout.cshtml";

}

@section HeaderRight{
    <form class="d-flex">
        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
        <button class="btn btn-outline-success" type="submit">Search</button>
    </form>
}



<div class="container">
    <h1 class="mt-5">Sticky footer with fixed navbar</h1>
    <p>Members of week @Model.NewMembersCount</p>
    <img src="~/Images/foundationLogo.jpeg" width="200" />

    @await Html.PartialAsync("~/Views/Shared/_Memberlist.cshtml", Model.Members)
</div>


